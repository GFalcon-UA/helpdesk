<?xml version="1.0" encoding="UTF-8" ?>
<Configuration status="WARN">
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS ZZZ} [%p] %c : %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <asyncRoot level="INFO">
            <AppenderRef ref="console"/>
        </asyncRoot>
    </Loggers>
</Configuration>